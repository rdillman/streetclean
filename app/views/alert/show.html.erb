<table>
  <tr>
    <th>Alarm Send Time</th>
    <th>Clean Time</th>
    <th>Content</th>
  </tr>
 
<% @alarms.each do |alarm| %>
  <tr>
    <td><%= alarm.send_time%></td>
    <td><%= alarm.clean_time %></td>
    <td><%= alarm.location %></td>
	<td><%=link_to "delete", :controller => "alert", :action => "kill", :q => alarm.id, :method => "get"%>
  </tr>
<% end %>
</table>